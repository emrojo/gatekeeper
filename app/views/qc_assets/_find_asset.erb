<%= form_tag({}, {:id=>'initial_form'}) do %>

  <div id="swipecard_holder">
    <%= render :partial => 'form_elements/user_swipecard' %>
  </div>

  <div class="form-group barcode-group">
    <label for="asset_barcode" class="col-sm-5 control-label">Plate or tube barcode</label>
    <div class="input-group col-sm-6">
      <%= input_glyph('barcode') %>
      <input type="text" class="form-control" id="initial_asset_barcode" name="asset_barcode" placeholder="Scan barcode" required="required">
    </div>
  </div>

  <div class="feedback"></div>

<% end %>
